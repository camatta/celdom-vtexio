{
  "flex-layout.col#payments-flags": {
    "title": "Logos",
    "props": {
      "blockClass": "payments-flags-content",
      "preventVerticalStretch": true
    },
    "children": ["flex-layout.row#payments-content"]
  },

  "flex-layout.row#payments-content": {
    "props": {
      "blockClass": "payments-content-container",
      "colSizing": "auto"
    },
    "children": ["flex-layout.col#payments-content"]
  },
  "flex-layout.col#payments-content": {
    "props": {
      "blockClass": "payments-content",
      "preventVerticalStretch": true
    },
    "children": [
      "image#visa",
      "image#master",
      "image#amex",
      "image#diners",
      "image#elo",
      "image#pix",
      "image#boleto"
    ]
  },

  "image#visa": {
    "props": {
      "src": "assets/flags/payment/visa.svg",
      "alt": "Visa",
      "title": "Visa"
    }
  },

  "image#master": {
    "props": {
      "src": "assets/flags/payment/master.svg",
      "alt": "MasterCard",
      "title": "MasterCard"
    }
  },

  "image#amex": {
    "props": {
      "src": "assets/flags/payment/amexlogo.svg",
      "alt": "Amex",
      "title": "Amex"
    }
  },

  "image#diners": {
    "props": {
      "src": "assets/flags/payment/diners.svg",
      "alt": "Diners",
      "title": "Diners"
    }
  },

  "image#elo": {
    "props": {
      "src": "assets/flags/payment/elo.svg",
      "alt": "Elo",
      "title": "Elo"
    }
  },

  "image#pix": {
    "props": {
      "src": "assets/flags/payment/pix.svg",
      "alt": "Pix",
      "title": "Pix"
    }
  },

  "image#boleto": {
    "props": {
      "src": "assets/flags/payment/boleto.svg",
      "alt": "Boleto",
      "title": "Boleto"
    }
  }
}
