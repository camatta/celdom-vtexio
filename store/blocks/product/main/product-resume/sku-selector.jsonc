{
  "flex-layout.row#sku-selector": {
    "props": {
      "blockClass": "sku-selector-container"
    },
    "children": ["flex-layout.col#sku-selector"]
  },

  "flex-layout.col#sku-selector": {
    "props": {
      "blockClass": "sku-selector-content"
    },
    "children": ["sku-selector"]
  },
  "sku-selector": {
    "props": {
      "blockClass": ["pdp"],
      "visibility": "more-than-one",
      "visibleVariations": ["Alimentação", "Diâmetro da Rosca", "cor", "Tamanho da Fita", "Tamanho"],
      "thumbnailImage": ["azul", "preto", "rosa", "verde", "vermelho", "amarelo", "bege"]
    } 
  }
}
